header.header
  .inner-logo
    a(href=`/${pathAdmin}/dashboard`)
      span
        img(src=websiteInfo.logo, alt="", style="width:90px")
  .inner-wrap
    .inner-account
      .inner-avatar
        img(src=account.avatar?account.avatar:"" , alt="")
      .inner-text
        .inner-name #{account.fullName? account.fullName: "" }
        .inner-role #{role.name ? role.name: ""}
    button.inner-button-menu
      <i class="fa-solid fa-bars"></i>